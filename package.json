{
	"name": "@egjs/persist",
	"version": "2.6.0",
	"description": "Provide cache interface to handle persisted data among history navigation.",
	"main": "dist/persist.js",
	"module": "dist/persist.esm.js",
	"es2015": "dist/persist.esm.js",
	"types": "./index.d.ts",
	"sideEffects": false,
	"scripts": {
		"start": "webpack-dev-server --open",
		"build": "rollup -c",
		"build:webpack": "webpack --env production",
		"test": "karma start",
		"test:chrome": "karma start --chrome",
		"lint": "eslint src",
		"jsdoc": "rm -rf ./doc && jsdoc -c jsdoc.json",
		"coverage": "npm run simulate:node && karma start --coverage",
		"simulate:node": "npm run build && node ./dist/persist.js",
		"release": "node config/release.js",
		"demo:start": "npm run demo:prebuild-latest && npm run demo:updateVersion && jekyll serve --host=0.0.0.0 -s demo",
		"demo:build": "npm run demo:updateVersion && jekyll build -s demo",
		"demo:prebuild-version": "cpx 'dist/**/*' demo/release/$npm_package_version/dist --clean && cpx 'doc/**/*' demo/release/$npm_package_version/doc --clean",
		"demo:prebuild-latest": "cpx 'dist/**/*' demo/release/latest/dist --clean && cpx 'doc/**/*' demo/release/latest/doc --clean",
		"demo:deploy": "npm run build && npm run jsdoc && npm run demo:prebuild-version && npm run demo:prebuild-latest && npm run demo:build && gh-pages -d demo/_site --add --remote upstream",
		"demo:updateVersion": "echo $npm_package_version > demo/_data/version.yml",
		"demo:setup": "cpx 'node_modules/@egjs/common-demo/**/*' 'demo/' && rm demo/package.json",
		"coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
		"prepush": "npm run lint",
		"commitmsg": "node config/validate-commit-msg.js"
	},
	"author": {
		"name": "NAVER Corp."
	},
	"keywords": [
		"persist",
		"storage",
		"session",
		"local",
		"sessionStorage",
		"localstorage",
		"history",
		"navigtation"
	],
	"namespace": {
		"eg": "eg"
	},
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.1.6",
		"@babel/plugin-proposal-class-properties": "^7.1.0",
		"@babel/plugin-transform-object-assign": "^7.0.0",
		"@babel/preset-env": "^7.1.6",
		"@egjs/common-demo": "github:naver/egjs#common-demo",
		"babel-eslint": "^10.0.1",
		"babel-loader": "^8.0.4",
		"babel-plugin-add-module-exports": "^0.2.1",
		"babel-plugin-no-side-effect-class-properties": "0.0.6",
		"babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
		"babel-plugin-transform-es3-property-literals": "^6.22.0",
		"babel-plugin-transform-object-assign": "^6.22.0",
		"chai": "^4.1.1",
		"chalk": "^2.1.0",
		"clean-webpack-plugin": "^0.1.16",
		"coveralls": "^2.13.1",
		"cpx": "^1.5.0",
		"egjs-jsdoc-template": "1.3.3",
		"eslint": "^4.7.1",
		"eslint-config-naver": "^1.0.1",
		"eslint-loader": "^1.9.0",
		"eslint-plugin-import": "^2.7.0",
		"fs-extra": "^4.0.1",
		"gh-pages": "^1.0.0",
		"husky": "^0.14.3",
		"inject-loader": "^3.0.1",
		"istanbul-instrumenter-loader": "^3.0.0",
		"jsdoc": "^3.6.6",
		"karma": "^1.7.0",
		"karma-chai": "^0.1.0",
		"karma-chrome-launcher": "^2.2.0",
		"karma-coverage-istanbul-reporter": "^1.3.0",
		"karma-mocha": "^1.3.0",
		"karma-mocha-reporter": "^2.2.3",
		"karma-sinon": "^1.0.5",
		"karma-sourcemap-loader": "^0.3.7",
		"karma-webpack": "^2.0.4",
		"lite-fixture": "^1.0.2",
		"mocha": "^3.5.0",
		"rollup": "^0.67.3",
		"rollup-plugin-babel": "^4.0.3",
		"rollup-plugin-replace": "^2.1.0",
		"rollup-plugin-uglify": "^6.0.0",
		"sinon": "^3.0.0",
		"string-replace-webpack-plugin": "0.1.3",
		"sync-exec": "^0.6.2",
		"uglifyjs-webpack-plugin": "^0.4.6",
		"webpack": "^3.4.1",
		"webpack-dev-server": "^2.6.1",
		"webpack-merge": "^4.1.0",
		"write-file-webpack-plugin": "^4.1.0"
	}
}
